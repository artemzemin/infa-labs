program simestrovaya;
    var
        x,y:real;

    begin
        writeln('Введите значения x:');
        readln(x);
        if x > 2 then
            y:= sin(pi * x)
        else if (x >= -2) and (x <= 2) then
            y:=cos(pi * x)
        else if x < -2 then
            y:=sqr(x)+5;
        writeln('y=', y:6:2);
    end.
